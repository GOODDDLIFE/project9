#define _CRT_SECURE_NO_WARNINGS
#include<stdio.h>
#define x 2

int main() {

   int a[x][x];
   int b[x][x];
   int c[x][x];

   printf("a행렬에 들어갈 숫자를 %d개 입력하시오\n", x * x);

   for (int i = 0; i < x; i++) {

      for (int j = 0; j < x; j++) {

         scanf("%d", &a[i][j]);

      }
   }


   printf("b행렬에 들어갈 숫자를 %d개 입력하시오\n", x * x);

   for (int i = 0; i < x; i++) {

      for (int j = 0; j < x; j++) {

         scanf("%d", &b[i][j]);

      }
   }
   for (int i = 0; i < x; i++) {
      for (int j = 0; j < x; j++) {
         for (int k = 0; k < x; k++) {
            c[i][j] += a[i][k] * b[k][j];
         }
      }
   }

   printf("두 행렬의 곱행렬은 \n");

   for (int i = 0; i < x; i++) {
      for (int j = 0; j < x; j++) {

         printf("%d  ", c[i][j]);

      }
      printf("\n");
   }

}#define _CRT_SECURE_NO_WARNINGS
#include<stdio.h>
#define x 2

int main() {

   int a[x][x];
   int b[x][x];
   int c[x][x];

   printf("a행렬에 들어갈 숫자를 %d개 입력하시오\n", x * x);

   for (int i = 0; i < x; i++) {

      for (int j = 0; j < x; j++) {

         scanf("%d", &a[i][j]);

      }
   }


   printf("b행렬에 들어갈 숫자를 %d개 입력하시오\n", x * x);

   for (int i = 0; i < x; i++) {

      for (int j = 0; j < x; j++) {

         scanf("%d", &b[i][j]);

      }
   }
   for (int i = 0; i < x; i++) {
      for (int j = 0; j < x; j++) {
         for (int k = 0; k < x; k++) {
            c[i][j] += a[i][k] * b[k][j];
         }
      }
   }

   printf("두 행렬의 곱행렬은 \n");

   for (int i = 0; i < x; i++) {
      for (int j = 0; j < x; j++) {

         printf("%d  ", c[i][j]);

      }
      printf("\n");
   }

}
